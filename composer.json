{
  "name": "compolomus/rssreader",
  "description": "rssreader",
  "license": "MIT",
  "keywords": [
    "rssreader"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Koenig",
      "email": "compolom@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Compolomus\\RssReader\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-dom": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-coding-standard": "^1.0",
    "larapack/dd": "^1.1",
    "symfony/dotenv": "^6.1"
  },
  "suggest": {
    "ext-xdebug": "^3.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  },
  "minimum-stability": "stable"
}
